<div class="grid-container">
  <header class="header">
    <div class="container">
      <button class="collapsible">Update</button>
      <div class="content">
        <div class="close_modal">X</div>
        <form action="/api/projects" enctype="multipart/form-data" method="POST">
          <div class="row">
            <label for="title">
              <h3>title</h3>
            </label>
            <input type="text" name="title" />
          </div>
          <div class="row">
            <label for="description">
              <h3>description</h3>
            </label>
            <input type="text" name="description" />
          </div>
          <div class="row">
            <label for="description">
              <h3>category</h3>
            </label>
            <input type="text" name="category" />
          </div>
          <div class="row">
            <label for="location">
              <h3>location</h3>
            </label>
            <input type="text" name="location" />
          </div>
          <div class="row">
            <label for="oName">
              <h3>owner</h3>
            </label>
            <input type="text" name="oName" />
          </div>
          <div class="row">
            <label for="cName">
              <h3>collaborator</h3>
            </label>
            <input type="text" name="cName" />
          </div>
          <div class="row" id="fileUpload">
            <br>
            <input type="file" name="myFile" />
            <br>
            <hr>
            <input class="submitBtn" type="submit" value="send ->" />
          </div>
        </form>
      </div>
      <button class="collapsibleAuth">login</button>
      <div class="content">
        <form action="/api/projects" enctype="multipart/form-data" method="POST">
          <label for="username">User
            <input type="username">
          </label>
          <label for="password">Pass
            <input type="password">
          </label>
        </form>
      </div>
    </div>
  </header>

  <sidebar class="sidebar">
    <p contenteditable="true">notes</p>
  </sidebar>
<!--****************************************************************************-->
<!--****************************AUDIO STUFF*************************************-->
<!--****************************************************************************-->

  <div class="wrapper">
       <section class="main-controls">
            <canvas class="visualizer" height="60px"></canvas>
            <div id="buttons">
                <button class="record"></button>
                <button class="stop"></button>
            </div>
        </section>

        <section class="sound-clips">
        </section>

    </div>
    
<!--****************************************************************************-->
<!--*****************************END AUDIO STUFF********************************-->
<!--****************************************************************************--> 
  <main class="main">
    <div class="showPlayer">
      <div class="container">
        <div class="leftCircle">
        </div>
        <div class="rightCircle">
        </div>
        <div class="transport">
          <div class="player">
            <audio class="player_video viewer" src=""></audio>
            <div class="player_controls">
              <div class="progress">
                <div class="progress_filled"></div>
              </div>
              <button class="player_button toggle" title="Toggle Play">&#9658;</button>
              <input type="range" name="volume" class="player_slider" min="0" max="1" step="0.05" value="1">
              <input type="range" name="playbackRate" class="player_slider" min="0.5" max="2" step="0.1" value="1">
              <button data-skip="-10" class="player_button">« 10s</button>
              <button data-skip="25" class="player_button">25s »</button>
            </div>
          </div>
        </div>
      </div>
    </div>
</div>
</div>

</div>
</main>

